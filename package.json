{
  "name": "gita-odia",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:bn": "APP_LANG=bn expo start",
    "start:or": "APP_LANG=or expo start",
    "start:hi": "APP_LANG=hi expo start",
    "start:as": "APP_LANG=as expo start",
    "prebuild:bn": "APP_LANG=bn npx expo prebuild --clean",
    "prebuild:or": "APP_LANG=or npx expo prebuild --clean",
    "prebuild:hi": "APP_LANG=hi npx expo prebuild --clean",
    "prebuild:as": "APP_LANG=as npx expo prebuild --clean",
    "android:bn": "APP_LANG=bn expo run:android",
    "android:or": "APP_LANG=or expo run:android",
    "android:hi": "APP_LANG=hi expo run:android",
    "android:as": "APP_LANG=as expo run:android",
    "ios:bn": "APP_LANG=bn expo run:ios",
    "ios:or": "APP_LANG=or expo run:ios",
    "ios:hi": "APP_LANG=hi expo run:ios",
    "ios:as": "APP_LANG=as expo run:ios",
    "build:bn": "npm run prebuild:bn && npm run android:bn",
    "build:or": "npm run prebuild:or && npm run android:or",
    "build:hi": "npm run prebuild:hi && npm run android:hi",
    "build:as": "npm run prebuild:as && npm run android:as",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/cli": "^54.0.10",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.7",
    "expo-build-properties": "^0.14.8",
    "expo-constants": "^18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~8.0.8",
    "expo-localization": "^17.0.7",
    "expo-router": "~6.0.4",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~15.0.7",
    "i18n-js": "^4.5.1",
    "i18next": "^25.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.0.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^15.7.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
